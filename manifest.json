{
  "description": "SNMP plugin",
  "gendevices": 0,
  "smart": 1,
  "restartOnChannelsChange": 1,

  "channel_popup": {
    "add": [
      { "id": "channel_read", "title": "$ChannelRead" },
      { "id": "channel_write", "title": "$ChannelWrite" }
    ],
    "copypaste": true,
    "delete": true
  },

  "channel_folder_popup": {
    "add": [
      { "id": "folder", "title": "$NewFolder" },
      { "id": "channel_folder", "title": "$NewNode" },
      { "id": "channel_read", "title": "$ChannelRead" },
      { "id": "channel_write", "title": "$ChannelWrite" }
    ],
    "browse": {
      "variant": "fields",
      "fields": [
        { "prop": "host", "title": "Host", "type": "text" },
        { "prop": "port", "title": "Port", "type": "text" },
        { "prop": "oid", "title": "Oid", "type": "input", "default":"1.3.6.1.2.1.2.2" }
      ]
    },
    "copypaste": true,
    "delete": true
  },

  "default_channel_folder": {
    "r": 1,
    "chan": "$NewNode",
    "host": "127.0.0.1",
    "port": 161,
    "trap_port": 162,
    "community": "public",
    "transport": "udp4",
    "version": 0
  },

  "default_channel_read": {
    "r": 1,
    "chan": "ch_1",
    "type": "get",
    "get_oid": "1.3.6.1.2.1.1.5.0",
    "table_oid": "1.3.6.1.2.1.2.2",
    "trap_oid": "",
    "interval": 60,
    "dn": "",
    "parse": "String(value)",
    "number": false
  },

  "default_channel_write": {
    "chan": "cw_1",
    "w": 1,
    "act": "on",
    "oid": "1.3.6.1.4.1.2.6.2.2.1.2.1",
    "type": "Integer",
    "value": "1"
  }
}
